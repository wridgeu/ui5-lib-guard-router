{
	"_version": "2.0.0",
	"sap.app": {
		"id": "demo.app",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "UI5 Enhanced Router Demo",
		"dataSources": {
			"auth": {
				"type": "JSON",
				"uri": "model/auth.json"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"dependencies": {
			"minUI5Version": "1.144.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"ui5.guard.router": {}
			}
		},
		"models": {
			"auth": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "auth"
			}
		},
		"rootView": {
			"viewName": "demo.app.view.App",
			"type": "XML",
			"id": "appView"
		},
		"routing": {
			"config": {
				"routerClass": "ui5.guard.router.Router",
				"type": "View",
				"viewType": "XML",
				"path": "demo.app.view",
				"controlId": "appContainer",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"name": "home",
					"pattern": "",
					"target": "home"
				},
				{
					"name": "protected",
					"pattern": "protected",
					"target": "protected"
				},
				{
					"name": "forbidden",
					"pattern": "forbidden",
					"target": "forbidden"
				}
			],
			"targets": {
				"home": {
					"name": "Home",
					"id": "homeView",
					"level": 1
				},
				"protected": {
					"name": "Protected",
					"id": "protectedView",
					"level": 2
				},
				"forbidden": {
					"name": "Forbidden",
					"id": "forbiddenView",
					"level": 2
				}
			}
		}
	}
}
